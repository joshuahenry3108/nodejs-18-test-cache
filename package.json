{
  "name": "nodejs-18-typescript",
  "version": "1.0.0",
  "main": "app.js",
  "engines": {
    "node": "18.20.7"
  },
  "scripts": {
    "cls": "clear",
    "start:dev": "npm run cls && env-cmd -f .env nodemon",
    "build": "tsc && tsc-alias && ls && cp .sequelizerc build/",
    "start": "node app.js",
    "migration": "npx sequelize-cli db:migrate --env=production",
    "undo:migration": "npx sequelize-cli db:migrate:undo --env=production",
    "create:migration": "npx sequelize-cli migration:generate --name auto-generated-migration --env=production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.7.9",
    "cls": "^0.1.5",
    "dotenv": "^16.4.7",
    "env-cmd": "^11.0.0",
    "express": "^4.21.2",
    "ioredis": "^5.8.1",
    "mysql2": "^3.12.0",
    "nodemon": "^3.1.10",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
